    func hideLoading()
    func showError(message: String)
}

// MARK: - Presenter Protocol

protocol {{ name }}PresenterProtocol: AnyObject {
    var view: {{ name }}ViewProtocol? { get set }
    var interactor: {{ name }}InteractorInputProtocol? { get set }
    var router: {{ name }}RouterProtocol? { get set }
    
    func viewDidLoad()
}

// MARK: - Interactor Input Protocol

protocol {{ name }}InteractorInputProtocol: AnyObject {
    var output: {{ name }}InteractorOutputProtocol? { get set }
    
